				<Optimize>False</Optimize>
			</BundleResource>
			<BundleResource Include="$(_MLKitTextKoreanResourcesFolder)tflite_langid.tflite" Visible="False">
				<LogicalName>KoreanOCRResources.bundle\tflite_langid.tflite</LogicalName>
